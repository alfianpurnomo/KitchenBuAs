}
a.btn-box-popup {
	position:absolute;
	top:30px;
	right:15px;
	padding:3px 15px;
	text-transform:uppercase;
	text-decoration:none;
	color:#006800;
	border:1px solid #006800;
}
a.btn-box-popup:hover {
	color:#fff;
	background:#006800;
	border:1px solid #006800;
}
.box-ctn-images .modal-content {
	background-color:transparent !important;
	-webkit-box-shadow: none !important;
	-moz-box-shadow: none !important;
	box-shadow: none !important;
	border:none !important;
	border-radius:0px !important;
	-moz-border-radius:0px !important;
	-webkit-border-radius:0px !important;
}
.box-ctn-images .modal-content .modal-body {
	padding:0px !important;	
	z-index:-2 !important;
}
.box-ctn-images .modal-content .modal-body img {
	max-width:100% !important;
	width:100% !important;
	border:none !important;	
}
.box-ctn-images .modal-content .modal-header {
	border:none !important;
	padding:0 0 20px 0 !important;
	min-height:34px !important;
	z-index:2 !important;
}
.box-ctn-images .modal-content .close {
	color:#fff !important;
	opacity:100 !important;	
	font-size:36px !important;
	margin-top:-10px !important;
	z-index:2;
}
.box-ctn-images .nav > li > a {
	padding:5px 10px;
	font-size:13px;
	text-transform:uppercase;
	margin-bottom:5px;
	border:1px solid #fff;
	color:#fff;
}
.box-ctn-images .nav > li > a:hover , .box-ctn-images .nav > li.active > a{
	color:#f0ed00;
	border:1px solid #f0ed00;
	background:transparent;
}
.blur {
	-webkit-filter: blur(5px) grayscale(50%);
    -moz-filter: blur(5px) grayscale(50%);
    -o-filter: blur(5px) grayscale(50%);
    -ms-filter: blur(5px) grayscale(50%);
    filter: blur(5px) grayscale(50%);
}
* { border:0; margin:0; padding:0; }
