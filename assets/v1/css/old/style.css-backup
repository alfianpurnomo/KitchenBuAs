body{
    background: url(../images/bg.png) repeat;
    margin-bottom: -250px;
    color:#4d4d4d;
}
h1{    
    text-transform: uppercase;
}
h1.header, h2.header, h3.header, h4.header{
    color: #006800;
    text-transform: uppercase;
    padding: 10px 0;
}
h1.header {
	font-size:30px;
	line-height:38px;
	margin:25px 0 0 0;
}
h3.header {
	font-size:18px;
	line-height: 24px;
	margin:0 0 10px 0;
}
#content{
    min-height: 300px;
    padding-bottom: 25px;
}
#slideContainer{
	/*height:353px;*/
    margin-bottom: 20px;
}
#slideContainer img {
	height:353px;
}
#big-header{
    background: #fcfcfc;
    height: 80px;
    border-bottom: 4px solid #b1d249;
    margin-bottom: 0;
}
#big-header, #big-header a{
    text-transform: uppercase;
}
a.navbar-brand{
    padding-top: 6px;
    padding-bottom: 0;
}
#topmenu > .navbar-nav > li > a{
    padding-bottom: 31px;
    padding-top: 26px;
    padding-left: 12px;
    padding-right: 12px;
}
#topmenu > .navbar-nav > li > a:hover, #topmenu > .navbar-nav > li > a:focus, #topmenu > .navbar-nav > li.dropdown.active {
	color:#b1d249;
	/*font-weight:bold;*/
	background:url(../images/nav.hover.png) no-repeat center bottom;
}
.navbar-default{
    border: none;
}
.navbar-default .navbar-nav > li > a,.navbar-default .navbar-nav > li > a:hover, .navbar-default .navbar-nav > li > a:focus{
    color: #006500;
}
.navbar-default .nav li.dropdown.open > .dropdown-toggle, 
.navbar-default .nav   li.dropdown.active > .dropdown-toggle, 
.navbar-default .nav li.dropdown.open.active > .dropdown-toggle {
    color: #b1d249;
    /*font-weight: bold;*/
 }
.navbar .nav li.dropdown.open > .dropdown-toggle, 
.navbar .nav li.dropdown.active > .dropdown-toggle, 
.navbar .nav li.dropdown.open.active > .dropdown-toggle {
    background:none;
    color: #b1d249;
 }
.dropdown-menu{
    background: none;
    border:none;
    padding: 0;
}
.dropdown-menu li a{
    background: rgba(255,251,0, 0.7);
    color: #006600;
    padding: 7px 70px 7px 20px;
}
.dropdown-menu li a:hover{
    color:#FFF;
    background: rgba(0,111,0, 0.7);
}
/*********** FOOTER ***********/
ul.menu-footer {margin:0;}
.footer{
    height: auto;
    padding-top:9px;
    padding-bottom: 10px;
    border-top: 5px solid #b1d249;
    background: #006900;
}
.footer{
    color: #FFF;
}
.footer h1,.footer h2,.footer h3,.footer h4, .copyright{
    color: #f0ed00;
}
.footer ul{
    list-style: none;
    color: #FFF;
    margin-bottom:0;
}
.footer a{
    color: #f0ed00;
    text-transform: uppercase;
}
.copyright{
    font-size:12px;
    display: block;
    clear:both;
    padding-top:40px;
}
.menu-footer{
    padding: 0;
}
.menu-footer a{
    padding: 5px 0;
    display: block;
}
@media (min-width: 992px) {
  .custom-map {
    width: 20.667%;
  }
  .custom-socmed{
        width: 29.333%;
        padding-right:0;
    }
}
.custom-map{
    text-align: center;
    padding-left:12px;
}
.custom-map .iframeContainer{
    margin-top:10px;
    margin-left: auto;
    margin-right: auto;
    overflow:hidden;
    border-radius: 50%;
    -moz-border-radius: 50%;
    -webkit-border-radius: 50%;
    -khtml-border-radius: 50%;
    position: relative;
    z-index: 9;
    width:180px;
    height: 180px;
    border:3px solid #b1d249;
}
.custom-map iframe{
    width:180px;
    height: 180px;
}
.call-center{
    font-size: 13px;
}
.call-center [class*="sprite-"]{
    position: absolute;
    left: 15px;
}
.call-center li{
    margin-bottom: 12px;
    padding-left: 35px;
}
.call-center li:last-child {
	margin-bottom:0;
}
.socmed-header{
    color: #f0ed00;
    margin-bottom: 20px;
    margin-top: 10px;
    font-size:17px;
    font-weight: bold;
}
/********* SPRITE ***********/
[class*="sprite-"]{
    background: url(../images/sprite.png);
}
.sprite-logofooter{
    background-position: -9px -122px;
    height: 77px;
    width: 140px;
}
.sprite-logo{
    background-position: -10px -2px;
    height: 63px;
    width: 130px;
}
.sprite-fb{
    background-position: -158px -9px;
    height: 47px;
    width: 47px;
}
.sprite-tw{
    background-position: -215px -9px;
    height: 47px;
    width: 47px;
}
.sprite-yt{
    background-position: -270px -9px;
    height: 47px;
    width: 47px;
}
.sprite-message{
    background-position: -100px -76px;
    height: 17px;
    width: 26px;
}
.sprite-location{
    background-position: -39px -74px;
    height: 28px;
    width: 21px;
}
.sprite-phone{
    background-position: -71px -76px;
    height: 22px;
    width: 18px;
}
.sprite-left{
    background-position: -216px -75px;
    height: 67px;
    width: 33px;
    position: absolute;
    top: 50%;
    margin-top: -35px;
}
.sprite-right{
    background-position: -158px -75px;
    height: 67px;
    width: 33px;
    position: absolute;
    top: 50%;
    margin-top: -35px;
    right:0;
}

/************ CUSTOM BOOTSTRAP CAROUSEL ************/
.carousel-control.left{
    background: none;
}
.carousel-control.right{
    background: none;
}
.carousel-indicators{
    bottom: -35px !important;
    right: -5px;
    width: auto;
    left: auto;
    margin-left: auto;
}
.carousel-indicators .active{
    background: #006800;
    height: 15px;
    width: 15px;
    margin: 0 5px;
}
.carousel-indicators li{
    height: 15px;
    width: 15px;
    border: 2px solid #006800;
    margin: 0 5px;
}
.carousel-indicators li:last-child{
    margin: 0 5px;
}
/****** EOF CAROUSEL *****/
.customer{
    min-height: 500px;
    padding-top: 0;
}
.customer h1.header {
	margin-bottom:20px;
}
.customer img {
	margin:0px auto;
	margin-bottom:15px;
}
.panel-group .panel{
    border-radius: 17px;
    background: #f0ed00;
    box-shadow: none;
}
.panel-default{
    border:none;
    margin-bottom: 10px;
}
.panel-default > .panel-heading{
    border-radius: 15px;
    background: #006800;
    border:none;
    color: #FFF;
}
.panel-group .panel-heading + .panel-collapse > .panel-body{
    border:none;
}
.panel-title > a{
    text-align: center;
    display: block
}
.panel-title > a:hover{
    text-decoration: none;
}

/********* CUSTOM SCROLLBAR **********/
.bigscroll{
    height: 200px;
}
.bigscroll-500{
    height:500px;
}
.scrollable.gray-skin .scroll-bar .thumb{
    background: #006800;
}
.scrollable.gray-skin .scroll-bar{
    border:none;
}
.scrollable.gray-skin .scroll-bar.vertical{
    width: 6px;
}

/******* CAROUFREDSEL ********/
.list_carousel {
	width: 100%;
}
.list_carousel ul {
	margin: 0;
	padding: 0;
	list-style: none;
	display: block;
}
.list_carousel li {
	font-size: 14px;
	color: #FFF;
	text-align: left;
	width: 80px;
	height: auto;
	padding: 0;
	margin: 0 10px 0 10px !important;
	margin-top: 30px;
	display: block;
	float: left;
	padding-left: 0 !important;
	/*margin-left: 25px;*/
}
.list_carousel li:first-child {
	/*margin-left: 0  !important;*/
}
.list_carousel li img {
	width: 100%;
	height: auto;
}
.list_carousel li .title {
	text-align: left;
        text-transform: uppercase;
        color: #006800;
        font-weight: bold;
        margin-top:5px;
}
.list_carousel.responsive {
	width: auto;
	margin-left: -5px;
	margin-right: 5px;
}
.clearfix {
	float: none;
	clear: both;
}
.carousel-pagination {
    text-align: right;
}
.carousel-pagination a {
    border-radius: 10px;
    -webkit-border-radius: 10px;
    -moz-border-radius: 10px;
    -khtml-border-radius: 10px;
    height: 15px;
    width: 15px;
    border: 2px solid #006800;
    margin: 0 5px;
    display: inline-block;
}
.carousel-pagination a.selected {
    background: #006800;
    height: 15px;
    width: 15px;
    margin: 0 5px;
    cursor: default;
}
.carousel-pagination a span {
    display: none;
}
.clearfix {
    float: none;
    clear: both;
}

/****** EOF CAROUFREDSEL *****/
.navleft {
	margin-bottom:10px;
}
#listPromo {
    margin: 0; 
    padding: 0;
    list-style: none;
}
#listPromo li.active a {font-weight:bold;color:#006700}
#listPromo li a{
    text-decoration: none;
    text-transform: uppercase;
    color: #555555;
    padding: 10px 0;
    border-top:1px solid #FFF;
    display: block;
}
#listPromo li a.active{
    font-weight: bold;
    color: #006800;
}
#promoContent{
    padding:20px;
    background: #FFF;
    color: #555;
}
#promoContent .title{ 
    text-transform: uppercase;
    font-weight: bold;
    color: #006800;
    margin-bottom: 10px;
}
