.zoom img {
	display: block;
}

.zoom img::selection { background-color: transparent; }
img.zoomImg {
	z-index:1 !important;
}
.sub-menu h3.ribbon {
	margin-bottom:20px;
}
.sub-menu h3.ribbon a {
	color:#f0ed00;
}
.sub-menu h3.ribbon a:hover {
	color:#fff;
	text-decoration:none;
}
